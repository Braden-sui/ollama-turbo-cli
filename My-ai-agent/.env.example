# Monorepo shared environment example
# Vite will read VITE_* variables from this file because envDir is set in agent-desk-pro/vite.config.ts
# Copy this to .env or .env.local at the repo root and adjust as needed.

# Frontend
VITE_API_BASE_URL=http://127.0.0.1:8000

# Backend (example, not exposed to frontend; do not prefix with VITE_)
# OLLAMA_HOST=http://127.0.0.1:11434
# LOG_LEVEL=INFO

# Mem0 (optional)
# Remote platform mode (requires MEM0_API_KEY)
# MEM0_API_KEY=
# MEM0_USER_ID=Braden
# MEM0_AGENT_ID=
# MEM0_APP_ID=
# MEM0_ORG_ID=
# MEM0_PROJECT_ID=
# MEM0_VERSION=v2
# MEM0_ENABLED=1
# MEM0_DEBUG=0
# MEM0_MAX_HITS=3
# MEM0_SEARCH_TIMEOUT_MS=200
# MEM0_TIMEOUT_CONNECT_MS=1000
# MEM0_TIMEOUT_READ_MS=2000
# MEM0_ADD_QUEUE_MAX=256
# MEM0_BREAKER_THRESHOLD=3
# MEM0_BREAKER_COOLDOWN_MS=60000
# MEM0_SHUTDOWN_FLUSH_MS=3000
# MEM0_SEARCH_WORKERS=2
# MEM0_IN_FIRST_SYSTEM=0           # Merge Mem0 context into first system message when 1
# MEM0_PROXY_MODEL=                # Optional rerank model (e.g., gpt-oss:20b) for Mem0 search results
# MEM0_PROXY_TIMEOUT_MS=1200       # Timeout for proxy reranker calls
# MEM0_RERANK_SEARCH_LIMIT=10      # Candidate memories to consider during rerank

# Local OSS/OpenMemory mode (no key required)
# MEM0_USE_LOCAL=0
# MEM0_LOCAL_CONFIG_PATH=
# MEM0_LOCAL_CONFIG_JSON=
# MEM0_OLLAMA_BASE_URL=http://127.0.0.1:11434
# MEM0_LLM_PROVIDER=ollama
# MEM0_LLM_MODEL=gpt-oss:120b
# MEM0_EMBEDDER_PROVIDER=ollama
# MEM0_EMBEDDER_MODEL=nomic-embed-text
# MEM0_VECTOR_PROVIDER=qdrant
# MEM0_VECTOR_HOST=127.0.0.1
# MEM0_VECTOR_PORT=6333
# MEM0_HISTORY_DB_PATH=
# MEM0_GRAPH_PROVIDER=
# MEM0_GRAPH_URL=
# MEM0_GRAPH_USERNAME=
# MEM0_GRAPH_PASSWORD=
