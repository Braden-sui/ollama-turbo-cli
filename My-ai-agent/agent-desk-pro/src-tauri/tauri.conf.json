{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Agent Desk Pro",
  "version": "0.1.0",
  "identifier": "com.agentdesk.pro",
  "build": {
    "beforeDevCommand": "npx pnpm@9 dev",
    "beforeBuildCommand": "npx pnpm@9 build",
    "devUrl": "http://127.0.0.1:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Agent Desk Pro",
        "width": 1280,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "transparent": false
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": ["msi"],
    "windows": {
      "wix": {
        "language": "en-US",
        "bannerPath": null,
        "dialogImagePath": null
      },
      "timestampUrl": null
    },
    "icon": []
  }
}
