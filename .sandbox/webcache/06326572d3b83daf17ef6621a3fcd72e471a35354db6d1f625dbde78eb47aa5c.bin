(function() {
    const MEDIA_AD_ID = 'D_sidebar_media_viewer_1';
    const overlayContainer = document.querySelector('.media-overlay-container');
    if (overlayContainer) {
        const observer = new MutationObserver(function(mutations) {
            // wait for element replace mutation (overlay open)
            if (
                mutations.length === 2 &&
                mutations[0].removedNodes.length === 1 &&
                mutations[1].addedNodes.length === 1
            ) {
                const adContainer = overlayContainer.querySelector('#media-overlay-ad');
                if (adContainer && adContainer.childElementCount === 0) {
                    inject(adContainer, MEDIA_AD_ID);
                }
            // wait for element remove mutation (overlay close)
            } else if (
                mutations.length === 1 &&
                mutations[0].removedNodes.length === 1
            ) {
                ayManagerEnv.destroy([MEDIA_AD_ID]);
                ayManagerEnv.refresh(null, { mode: 'visible' });
            }
        });
        observer.observe(overlayContainer, { subtree: false, childList: true });
    }
    function inject(target, code) {
        const script = document.createElement('script');
        script.type = 'text/javascript';
        script.appendChild(document.createTextNode(`window.ayManagerEnv.cmd.push(function() { ayManagerEnv.display("${code}"); });`));

        const wrapper = document.createElement('div');
        wrapper.setAttribute('data-ay-manager-id', code);
        wrapper.appendChild(script);

        target.appendChild(wrapper);
    }
})();
