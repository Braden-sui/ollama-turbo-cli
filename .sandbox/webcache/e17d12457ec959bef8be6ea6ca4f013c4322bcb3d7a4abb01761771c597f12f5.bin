\n    ...props,\n    children: _jsx(_createMdxContent, {\n      ...props\n    })\n  }) : _createMdxContent(props);\n}\nreturn {\n  default: MDXContent\n};\nfunction _missingMdxReference(id, component) {\n  throw new Error(\"Expected \" + (component ? \"component\" : \"object\") + \" `\" + id + \"` to be defined: you likely forgot to import, pass, or provide it.\");\n}\n"])</script><script>self.__next_f.push([1,"36:[[\"$\",\"$L37\",\"reference/how-exa-search-works\",{\"children\":[\"$\",\"$L38\",null,{\"isCustom\":false,\"children\":[[\"$\",\"$L39\",null,{}],[\"$\",\"div\",null,{\"className\":\"relative grow box-border flex-col w-full mx-auto px-1 lg:pl-[23.7rem] lg:-ml-12 xl:w-[calc(100%-28rem)]\",\"id\":\"content-area\",\"children\":[[\"$\",\"$L3a\",null,{}],[\"$\",\"$L3b\",null,{\"mobile\":true}],[\"$\",\"$L3c\",null,{}],[\"$\",\"div\",null,{\"className\":\"mdx-content relative mt-8 prose prose-gray dark:prose-invert\",\"data-page-title\":\"How Exa Search Works\",\"data-page-href\":\"/reference/how-exa-search-works\",\"children\":[[\"$\",\"$L3d\",null,{\"mdxSource\":{\"compiledSource\":\"$3e\",\"frontmatter\":{},\"scope\":{\"pageMetadata\":{\"title\":\"How Exa Search Works\",\"description\":\"Exa is a novel search engine that utilizes the latest advancements in AI language processing to return the best possible results.\",\"href\":\"/reference/how-exa-search-works\"},\"config\":{}}},\"mdxSourceWithNoJs\":{\"compiledSource\":\"$3f\",\"frontmatter\":{},\"scope\":{\"pageMetadata\":{\"title\":\"How Exa Search Works\",\"description\":\"Exa is a novel search engine that utilizes the latest advancements in AI language processing to return the best possible results.\",\"href\":\"/reference/how-exa-search-works\"},\"config\":{}}}}],\"$undefined\",\"$undefined\"]}],\"$L40\"]}]]}]}]]\n"])</script><script>self.__next_f.push([1,"41:I[7247,[\"3473\",\"static/chunks/891cff7f-2ca7d0df884db9d0.js\",\"4129\",\"static/chunks/7bf36345-5ba13855b95a82b2.js\",\"1725\",\"static/chunks/d3